{
  "name": "study",
  "version": "1.0.0",
  "description": "A native application built with NativeScript-Vue",
  "author": "figur8 <ismael_profissional@hotmail.com.br>",
  "license": "MIT",
  "scripts": {
    "appium": "node generate-screenshots.js",
    "android.build": "tns build android --bundle",
    "android.screenshots": "npm run appium -- --runType=android27",
    "ci.android.screenshots": "npm run appium -- --runType=android23 --sauceLab=true",
    "ci.ios.screenshots": "npm run appium -- --runType=ios-simulator103iPhone6",
    "deploy": "bash deploy.sh",
    "e2e": "mocha --opts ./e2e/config/mocha.opts",
    "ios.build": "tns build ios --bundle",
    "ios.screenshots": "npm run appium -- --runType=ios-simulator103iPhone6"
  },
  "nativescript": {
    "id": "org.nativescript.application",
    "templateVersion": "v2",
    "tns-ios": {
      "version": "6.0.1"
    },
    "tns-android": {
      "version": "6.1.2"
    }
  },
  "dependencies": {
    "@fusionauth/typescript-client": "^1.10.1",
    "@vue/devtools": "^5.0.6",
    "nativescript-android-utils": "^1.0.2",
    "nativescript-fontawesome": "^1.0.0",
    "nativescript-fonticon": "^2.0.0",
    "nativescript-geolocation": "^5.1.0",
    "nativescript-local-notifications": "^4.0.1",
    "nativescript-socketio": "^3.2.1",
    "nativescript-telephony": "^1.0.4",
    "nativescript-theme-core": "^1.0.6",
    "nativescript-toasty": "^1.3.0",
    "nativescript-unit-test-runner": "^0.7.0",
    "nativescript-vue": "^2.4.0",
    "nativescript-vue-devtools": "^1.2.0",
    "nativescript-vue-navigator": "^0.2.0",
    "nativescript-vue-router-ns": "^1.0.4",
    "request": "^2.88.0",
    "tns-core-modules": "^6.0.0",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/chai": "~4.1.7",
    "@types/mocha": "^7.0.2",
    "@types/node": "~10.12.18",
    "babel-loader": "^8.0.2",
    "chai": "4.2.0",
    "karma": "5.0.9",
    "karma-chai": "0.1.0",
    "karma-mocha": "1.3.0",
    "karma-nativescript-launcher": "0.4.0",
    "karma-webpack": "3.0.5",
    "mocha": "6.2.0",
    "mocha-junit-reporter": "~1.18.0",
    "mocha-multi": "~1.0.1",
    "mochawesome": "~3.1.2",
    "nativescript-dev-appium": "^6.1.3",
    "nativescript-dev-webpack": "^1.0.0",
    "nativescript-vue-template-compiler": "^2.0.0",
    "nativescript-worker-loader": "~0.9.0",
    "node-sass": "^4.9.2",
    "tns-platform-declarations": "^6.3.2",
    "typescript": "^3.2.4",
    "vue": "^2.5.22",
    "vue-class-component": "^7.1.0",
    "vue-loader": "^15.4.0"
  },
  "scripts": {
    "e2e": "node ./node_modules/nativescript-dev-appium/check-dev-deps.js &&  mocha --opts ./e2e/config/mocha.opts "
  }
}
